<div class='row'>
  <div class='col-md-1'>
    <button class='fill-all' {{action 'prevStory'}}>◀</button>
  </div>
  <div class='col-md-3'>
  </div>
  <div class='col-md-1'>
    <button class='fill-all' {{action 'toggleRead'}}>
      {{#if read}}●{{else}}○{{/if}}
    </button>
  </div>

  <div class='col-md-2'>
    <button class='fill-all' {{action 'toggleShowInIframe'}}>
      {{#if showInIframe}}
        RSS
      {{else}}
        Preview
      {{/if}}
    </button>
  </div>
  <div class='col-md-4'>
  </div>
  <div class='col-md-1'>
    <button class='fill-all' {{action 'nextStory'}}>▶</button>
  </div>
</div>
