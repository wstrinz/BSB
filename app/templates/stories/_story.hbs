<h3>
  <a target="_blank" {{bind-attr href="url"}}>
    {{title}}
  </a>
</h3>
<p>
  {{#link-to 'feeds.stories' feed.id}}
    {{feed.name}}
  {{/link-to}}
</p>
<p>{{author}}</p>
<p>{{timestamp}}</p>
<p><button {{action 'markUnread'}}>Mark Unread</button></p>
<p>
{{#if story_content}}
{{{story_content}}}
{{else}}
{{{summary}}}
{{/if}}
</p>
<p></p>
